CREATE TABLE users ( userid INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, username VARCHAR(200) NOT NULL, bitsid VARCHAR (50) NOT NULL, password VARCHAR(500) NOT NULL, salt VARCHAR(500) NOT NULL, make_time TIMESTAMP);
CREATE TABLE tags ( tagid INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, userid INT(6) NOT NULL, tag VARCHAR(500) NOT NULL, make_time TIMESTAMP );
CREATE TABLE posts ( postid INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, poster VARCHAR(500), created_time TIMESTAMP, status_type VARCHAR(50), message LONGTEXT NOT NULL, mid LONGTEXT NOT NULL, make_time TIMESTAMP );
CREATE TABLE bookmarks ( bookid INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, userid INT(6) NOT NULL, postid INT(6) NOT NULL, make_time TIMESTAMP );
